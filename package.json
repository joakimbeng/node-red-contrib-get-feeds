    "feed",
    "html",
    "rss",
    "atom"
  ],
  "author": {
    "name": "Joakim Carlstein",
    "email": "joakim@beng.se",
    "url": "http://joakim.beng.se"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joakimbeng/node-red-contrib-get-feeds/issues"
  },
  "homepage": "https://github.com/joakimbeng/node-red-contrib-get-feeds#readme",
  "xo": {
    "envs": [
      "node"
    ]
  },
